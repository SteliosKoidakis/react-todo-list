{
  "parser": "babel-eslint",
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "jasmine": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "prettier/prettier": ["error", { "singleQuote": true }],
    "object-curly-newline": "off",
    "comma-dangle": "off",
    "react/forbid-prop-types": "off",
    "arrow-parens": "off",
    "linebreak-style": "off"
  }
}
