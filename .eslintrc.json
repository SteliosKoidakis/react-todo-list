{
  "extends": ["airbnb", "prettier/react"],
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "jasmine": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "prettier/prettier": ["error", { "singleQuote": true }],
    "object-curly-newline": "off"
  }
}
